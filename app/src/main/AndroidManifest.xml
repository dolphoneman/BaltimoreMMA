<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.encounterfitness.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RockNRoll"
            android:label="@string/rockNroll"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Country"
            android:label="@string/country"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RhythmAndBlues"
            android:label="@string/rhythmNBlues"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Rap"
            android:label="@string/rap"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>