<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.encounterfitness.baltimoremma">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.encounterfitness.baltimoremma.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.encounterfitness.baltimoremma.Boxing"
            android:label="@string/boxing"
            android:parentActivityName="com.encounterfitness.baltimoremma.MainActivity"/>
        <activity android:name="com.encounterfitness.baltimoremma.BrazilianJiuJitsu"
            android:label="@string/bjj"
            android:parentActivityName="com.encounterfitness.baltimoremma.MainActivity"/>
        <activity android:name="com.encounterfitness.baltimoremma.KidFriendly"
            android:label="@string/kidfriendly"
            android:parentActivityName="com.encounterfitness.baltimoremma.MainActivity"/>
        <activity android:name="com.encounterfitness.baltimoremma.MuayThai"
            android:label="@string/muaythia"
            android:parentActivityName="com.encounterfitness.baltimoremma.MainActivity">
        </activity>
    </application>

</manifest>